{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
  },
  "rules": {
    "no-console": 0,
    "no-tabs": 0,
    "indent": ["error", "tab"],
    "import/no-unresolved": 0,
    "no-unused-vars": 0,
    "no-undef": 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0,
  },
}
